{
  "compilerOptions": {
    "target": "es6",
    "module": "NodeNext", // Sesuaikan module dengan NodeNext
    "moduleResolution": "NodeNext", // Sesuaikan moduleResolution dengan NodeNext
    "baseUrl": "./",
    "paths": {
      "*": ["./src/*"]
    }
  },
  "include": ["src/**/*.js"],
  "exclude": ["node_modules"]
}

